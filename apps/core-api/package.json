{
  "name": "core-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepare": "prisma generate",
    "test": "vitest run --coverage && rimraf coverage",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "test:db:start": "docker run -d --name aiew-test-db -e POSTGRES_USER=test -e POSTGRES_PASSWORD=test -e POSTGRES_DB=aiew_test -p 5433:5432 --tmpfs /var/lib/postgresql/data postgres:16-alpine && sleep 2",
    "test:db:stop": "docker rm -f aiew-test-db || true",
    "test:db:migrate": "DATABASE_URL=\"postgresql://test:test@localhost:5433/aiew_test\" prisma migrate deploy",
    "test:with-db": "pnpm test:db:start && pnpm test:db:migrate && pnpm test; pnpm test:db:stop",
    "start": "node dist/server.js",
    "build": "rimraf dist && tsc && tsc-alias",
    "dev": "nodemon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/oauth2": "^8.1.2",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@google-cloud/text-to-speech": "^6.3.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.0.0",
    "@prisma/extension-accelerate": "^3.0.0",
    "@sinclair/typebox": "^0.34.40",
    "axios": "^1.12.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.4.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "prisma": "7.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "fastify-tsconfig": "^3.0.0",
    "form-data": "^4.0.4",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
