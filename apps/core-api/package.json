{
  "name": "core-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepare": "prisma generate",
    "test": "npm run build && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register $(find test -name '*.test.ts')",
    "start": "node dist/server.js",
    "build": "tsc",
    "watch:ts": "tsc -w",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "nodemon dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/oauth2": "^8.1.2",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.12.0",
    "axios": "^1.10.0",
    "fastify": "^5.4.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1",
    "prisma": "^6.12.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "c8": "^10.1.3",
    "concurrently": "^9.2.0",
    "fastify-tsconfig": "^3.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  }
}
